{
  "name": "pseudo-ifts",
  "author": {
    "name": "Carolina Cappato"
  },
  "publisher": "cm-cappato",
  "displayName": "Pseudocodigo IFTS",
  "description": "Pseudocodigo IFTS 18 - Técnicas de Programación",
  "version": "1.0.4",
  "repository": {
    "url": "https://github.com/cmcappato/pseudo-ifts"
  },
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "pseudo-ifts",
        "aliases": [
          "pseudo-ifts"
        ],
        "extensions": [
          ".psc",
          ".algo",
          ".pseudo"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pseudo-ifts",
        "scopeName": "source.ifts",
        "path": "./syntaxes/pseudocodigo.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "pseudo-ifts",
        "path": "./snippets/snippets.json"
      }
    ]
  }
}
